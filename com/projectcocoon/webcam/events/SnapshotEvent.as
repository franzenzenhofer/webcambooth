package com.projectcocoon.webcam.events {		import flash.events.Event;	import flash.media.Video;    	public class SnapshotEvent extends Event {				public static const SNAPSHOT:String = "snapshot";				public var video:Video;	  		public function SnapshotEvent(type:String, vid:Video, bubbles:Boolean=false, cancelable:Boolean=false) {			super(type, bubbles, cancelable);			video = vid;		}	  		public override function clone():Event {			return new SnapshotEvent(type, video, bubbles, cancelable);	  	}			public override function toString():String {			return formatToString("SnapshotEvent", "type", "vid", "bubbles", "cancelable", "eventPhase");		}	     }}